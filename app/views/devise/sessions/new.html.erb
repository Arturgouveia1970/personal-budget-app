<h1 class="logs">Log In</h1>
</br>



<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
<form>
    <div class="form-outline mb-4">
      <%= f.email_field :email, autofocus: true, autocomplete: "email", placeholder: "Email", class: "form-control" %>
    </div>
    <hr />
    <div class="form-outline mb-4">
      <%= f.password_field :password, autocomplete: "new-password", placeholder: "Password(6 characters)", class: "form-control" %>
    </div>
    </br>
    <% if devise_mapping.rememberable? %>
      <div class="field">
        <%= f.check_box :remember_me, class: 'none' %>
        <%= f.label :remember_me, class: 'none' %>
      </div>
    <% end %>
    </br>
    <div class="actions">
      <%= f.submit "Log in", class: "btn btn-success" %>
    </div>
  <% end %>
</form>  

